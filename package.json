{
	"name": "@devprotocol/clubs-plugin-akiba",
	"version": "0.1.0-beta.5",
	"type": "module",
	"description": "Akiba theme plugin for clubs.",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*.d.ts",
		"dist/*.js",
		"!**/*.test.*",
		"src"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "vitest",
		"preview": "astro --root .preview/preview.ssr dev",
		"preview:static": "astro --root .preview/preview.static dev",
		"build": "rollup -c",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test --run && yarn prebuild && yarn build",
		"prepare": "husky",
		"typecheck": "astro check && tsc --noEmit && svelte-check && vue-tsc --noEmit --p tsconfig.vue.json"
	},
	"author": "Dev Protocol",
	"license": "MPL-2.0",
	"dependencies": {
		"@devprotocol/clubs-plugin-passports": "^0.1.0-beta.31",
		"@devprotocol/clubs-plugin-payments": "^0.2.1",
		"@devprotocol/dev-kit": "8.7.0",
		"@devprotocol/util-ts": "4.0.0",
		"@wagmi/core": "^1.4.1",
		"@web3modal/wagmi": "^3.5.7",
		"bignumber.js": "9.1.2",
		"fast-average-color": "^9.4.0",
		"ramda": "0.30.1",
		"sass": "1.82.0",
		"vue3-carousel": "^0.9.0"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^2.1.1 || ^3.0.0",
		"ethers": "^6.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.0",
		"@astrojs/svelte": "^6.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^4.0.0",
		"@devprotocol/clubs-core": "3.22.2",
		"@rollup/plugin-typescript": "12.1.1",
		"@tailwindcss/typography": "^0.5.10",
		"@types/dotenv": "8.2.3",
		"@types/node": "22.10.2",
		"@types/ramda": "0.30.2",
		"@typescript-eslint/eslint-plugin": "8.18.0",
		"@typescript-eslint/parser": "8.18.0",
		"astro": "^4.0.0",
		"dotenv": "16.4.7",
		"eslint": "8.57.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-functional": "6.6.3",
		"ethers": "6.13.4",
		"husky": "9.1.7",
		"prettier": "3.4.2",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.6.0",
		"rimraf": "6.0.1",
		"rollup": "4.28.1",
		"rollup-plugin-dts": "^6.1.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "3.4.16",
		"typescript": "5.6.3",
		"vitest": "^2.0.0",
		"vue": "3.5.13",
		"vue-tsc": "^2.0.0"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
